{
 "manifest_version": 2,
 "name": "International Anniversary",
 "description": "This extension displays the specific International Observance/Anniversary for the current day.",
 "version": "0.1",
 "browser_action": {
  "default_icon": "icon48.png",
  "default_popup": "popup.html",
  "default_title": "International Anniversary"
 },
 "icons": {
  "16": "icon16.png",
  "48": "icon48.png",
  "128": "icon128.png"
 },
 "permissions": [
  "activeTab",
  "https://ajax.googleapis.com/"
 ]
}
